{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "cast_spaces": {
            "space": "none"
        },
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "method": "one",
                "property": "none",
                "trait_import": "none",
                "case": "none"
            }
        },
        "fully_qualified_strict_types": true,
        "not_operator_with_successor_space": false
    }
}
